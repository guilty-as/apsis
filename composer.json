{
    "name": "guilty/apsis",
    "description": "APSIS PHP API Client",
    "keywords": [
        "guilty",
        "apsis"
    ],
    "homepage": "https://github.com/guilty-as/apsis",
    "license": "MIT",
    "authors": [
        {
            "name": "Helge Sverre",
            "email": "helgesverre@guilty.no",
            "homepage": "https://guilty.no",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "ramsey/uuid": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Guilty\\Apsis\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guilty\\Apsis\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Guilty\\Apsis\\ApsisServiceProvider"
            ],
            "aliases": {
                "Apsis": "Guilty\\Apsis\\ApsisFacade"
            }
        }
    }
}
