{
    "name": "guilty/apsis",
    "description": "APSIS API SDK",
    "keywords": [
        "guilty",
        "apsis"
    ],
    "homepage": "https://github.com/guilty-as/apsis",
    "license": "MIT",
    "authors": [
        {
            "name": "Helge Sverre",
            "email": "helgesverre@guilty.no",
            "homepage": "https://guilty.no",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Guilty\\Apsis\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guilty\\Apsis\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Guilty\\Apsis\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Apsis": "Guilty\\Apsis\\ApsisFacade"
            }
        }
    }
}
